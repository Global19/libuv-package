{
  "version": "1.9.0",
  "description": "Multi-platform support library with a focus on asynchronous I/O.",
  "licenseUrl": "https://raw.githubusercontent.com/aspnet/libuv-package/dev/content/License.txt",
  "authors": [
    "Libuv"
  ],
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/libuv-package"
    },
    "tags": [
      "libuv"
    ],
    "files": {
      "mappings": {
        "/": "../../content/License.txt",
        "runtimes/win7-x64/native/": "runtimes/win7-x64/native/*",
        "runtimes/win7-x86/native/": "runtimes/win7-x86/native/*",
        "runtimes/win7-arm/native/": "runtimes/win7-arm/native/*",
        "runtimes/osx/native/": "runtimes/osx/native/*",
        "runtimes/rhel-x64/native/": "runtimes/rhel-x64/native/*",
        "runtimes/debian-x64/native/": "runtimes/debian-x64/native/*",
        "runtimes/opensuse-x64/native/": "runtimes/opensuse-x64/native/*",
        "runtimes/fedora-x64/native/": "runtimes/fedora-x64/native/*"
      }
    }
  },
  "dependencies": {
    "Microsoft.NETCore.Platforms": "1.0.1",
    "Microsoft.AspNetCore.Internal.libuv-Darwin": {
      "version": "1.9.0-*",
      "type": "build"
    },
    "Microsoft.AspNetCore.Internal.libuv-Linux": {
      "version": "1.9.0-*",
      "type": "build"
    },
    "Microsoft.AspNetCore.Internal.libuv-Windows": {
      "version": "1.9.0-*",
      "type": "build"
    }
  },
  "frameworks": {
    "netstandard1.0": {}
  }
}